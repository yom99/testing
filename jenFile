pipeline {
    agent any

    stages {
        stage('initial') {
            steps {
               git 'https://github.com/yom99/testing.git'
            }
        }
         stage('insall npm') {
            steps {
               sh 'npm install'
            }
        }
         stage('run') {
            steps {
               sh 'npm test'
            }
        }
        
        
      }
}
